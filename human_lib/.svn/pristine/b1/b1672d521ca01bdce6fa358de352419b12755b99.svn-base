var bookService = (function() { 

	/** 도서 등록 */
	function bookregister(book, callback) {
		$.ajax({
		    type : "post",
			url : "/book/register",
			data : JSON.stringify(book),
			contentType : "application/json; charset=utf-8",
            success : function(result) {
                if(callback)
                callback(result);
            }
		})
	}
    return {
    	bookregister:bookregister,
    }
    
})();